#include <stdio.h>

#include <cmwpp.h>

int main(void)
{
    unsigned char to_serial[1024];
	unsigned char from_serial[1024];
	wpp_version *version = (wpp_version *)from_serial;

    for (int i = 0 ; i < 2 ; ++i) {
		wpp_encode_get_version(buf);
		wpp_decode_get_version(buf, version);
		printf("%c%c%c%c\n", version->start[0], version->start[1], version->start[2], version->start[3]);
		printf("%d\n", version->type);
		printf("%d\n", version->length);
		printf("%d\n", version->seq);
	}
    return 0;
}
